<h1>Clerk Registration Form</h1>
<section>
<%= form_with url: manage_clerks_path do |form|%>
    <p>Name:</p>
    <%= form.text_field :name %>
    <p>Email:</p>
    <%= form.email_field :email %>
    <p>Password</p>
    <%= form.password_field :password %><br>
    <%= form.submit "Add" %>

<% end %>
</section>