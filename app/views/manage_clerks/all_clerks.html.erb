<section class="Main-box Shadow">
<div>
    <p class="Hilight">Current Clerks</p>
    <table>
    <tr>
    <th class="Table-item">Name</th>
    <th class="Table-item">Email</th>
    </tr>
    <% @clerks.all.each do |clerk|%>
        <tr>
            <td class="Table-item"><%= link_to "#{clerk.name}", edit_manage_clerk_path(:id => clerk.id) , :method => :get %></td>
            <td class="Table-item"><%= clerk.email%></td>
        </tr>
    <% end %>
    </table>
</div>
<div>
    <br><%= link_to "Add new clerk" , new_manage_clerk_path , :class => "Content-link"%>
</div>